    		<element name="quantity" type="double" minOccurs="0"
    			maxOccurs="1">
    		</element>
    		<element name="unit" type="string" minOccurs="0" maxOccurs="1"></element>
    	</sequence>
    </complexType>

    <complexType name="TestSuiteFixture">
    	<sequence>
    		<choice minOccurs="1" maxOccurs="unbounded">
    			<element name="test" type="tns:TestFixture" minOccurs="0" maxOccurs="unbounded"></element>
    			<element name="test-group" type="tns:TestGroupFixture" minOccurs="0" maxOccurs="unbounded"></element>
    		</choice>
    	</sequence>
    </complexType>

    <complexType name="TestGroupFixture">
    	<sequence>
    		<element name="test" type="tns:TestFixture" minOccurs="1" maxOccurs="unbounded"></element>
