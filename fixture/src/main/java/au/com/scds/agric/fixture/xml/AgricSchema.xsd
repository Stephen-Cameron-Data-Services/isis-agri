    </complexType>


    <element name="batches" type="tns:BatchesFixture"></element>

    <complexType name="BatchesFixture">
    	<sequence>
    		<element name="batch" type="tns:BatchFixture" minOccurs="1" maxOccurs="unbounded"></element>
    	</sequence>
    </complexType>


    <complexType name="IngredientFixture">
    	<sequence>
    		<element name="name" type="string" minOccurs="1"
    			maxOccurs="1">
