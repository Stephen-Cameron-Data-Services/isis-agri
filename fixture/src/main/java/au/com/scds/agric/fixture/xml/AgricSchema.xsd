    		<element name="completed-order-line"
    			type="tns:CompletedOrderLineFixture" minOccurs="0"
    			maxOccurs="unbounded">
    		</element>
    		<element name="delivery" type="tns:DeliveryFixture" minOccurs="0" maxOccurs="1"></element>
    	</sequence>
    </complexType>

    <complexType name="NewOrderLineFixture">
    	<complexContent>
    		<extension base="tns:OrderLine">
    			<sequence>
    				<element name="added-on" type="dateTime"></element>
                    <element name="added-by" type="tns:PersonFixture" minOccurs="1" maxOccurs="1"></element>
                </sequence>
    		</extension>
    	</complexContent>
    </complexType>

    <complexType name="ClientFixture">
    	<complexContent>
    		<extension base="tns:Party">
    			<sequence>
    				<element name="order" type="tns:OrderFixture" minOccurs="0" maxOccurs="unbounded"></element>
    			</sequence>
    		</extension>
    	</complexContent>
    </complexType>

    <complexType name="DeliveryFixture">
    	<sequence>
    		<element name="delivery-date" type="dateTime" minOccurs="1" maxOccurs="1"></element>
    	</sequence>
    </complexType>

    <complexType name="TestFixture">
    	<sequence>
    		<element name="name" type="string" minOccurs="1"
    			maxOccurs="1">
